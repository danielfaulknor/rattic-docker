[loggers]
keys=root, gunicorn.error, gunicorn.access

[handlers]
keys=console

[formatters]
keys=generic

[logger_root]
level=INFO
handlers=console
propagate=1

[logger_gunicorn.error]
level=WARNING
handlers=console
propagate=1
qualname=gunicorn.error

[logger_gunicorn.access]
level=WARNING
handlers=console
propagate=1
qualname=gunicorn.access

[handler_console]
class=StreamHandler
formatter=generic
args=(sys.stdout, )

[formatter_generic]
class=logstash_formatter.LogstashFormatter
format={"extra":{"app": "rattic"}}
